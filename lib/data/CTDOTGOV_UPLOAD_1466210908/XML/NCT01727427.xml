  <brief_title>Prospective Study on the Treatment of Unsuspected Pulmonary Embolism in Cancer Patients</brief_title>
  <official_title>Prospective Study on the Treatment of Unsuspected Pulmonary Embolism in Cancer Patients</official_title>
  <sponsors>
    <lead_sponsor>
      <agency>G. d'Annunzio University</agency>
      <agency_class>Other</agency_class>
    </lead_sponsor>
  </sponsors>
  <source>G. d'Annunzio University</source>
